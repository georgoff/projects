void setup() {
// initialize the digital pin as an output.
// Pin 8 is our output pin
pinMode(8, OUTPUT);
}
void loop() {
digitalWrite(8, HIGH); // set the LED on
delay(100); // wait for a second
digitalWrite(8, LOW); // set the LED off
delay(100); // wait for a second
digitalWrite(9, HIGH);
delay(100);
digitalWrite(9, LOW);
delay(100);
digitalWrite(10, HIGH);
delay(100);
digitalWrite(10, LOW);
delay(100);
digitalWrite(11, HIGH);
delay(100);
digitalWrite(11, LOW);
delay(100);
digitalWrite(12, HIGH);
delay(100);
digitalWrite(12, LOW);
delay(100);
digitalWrite(13, HIGH);
delay(100);
digitalWrite(13, LOW);
delay(100);
}
